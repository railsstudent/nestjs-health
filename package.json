{
  "name": "nestjs-health",
  "version": "0.0.1",
  "description": "Create a NestJS health check library",
  "author": "Connie Leung",
  "license": "MIT",
  "bugs": "https://github.com/railsstudent/nestjs-health/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/railsstudent/nestjs-health.git"
  },
  "homepage": "https://github.com/railsstudent/nestjs-health#readme",
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "cp package.json ./dist && rm tsconfig.build.tsbuildinfo",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "cspell": "cspell --no-must-find-files lib/**/*.{ts,js}",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@nestjs/axios": "0.1.0",
    "@nestjs/bull": "0.6.1",
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/terminus": "9.1.2",
    "@nestjs/testing": "9.0.0",
    "@types/bull": "4.10.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "16.11.68",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "bull": "4.10.1",
    "cspell": "6.12.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-rxjs": "5.0.2",
    "eslint-plugin-sonarjs": "0.15.0",
    "husky": "8.0.1",
    "jest": "28.1.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">= 16"
  }
}
